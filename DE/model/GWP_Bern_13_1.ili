    CLASS Leitung
    EXTENDS Anlagenkataster_BaseClass =
      /** Das Attribut "Name_Nummer" ist eine Namensbezeichnung. Sie muss einheitlich verwendet werden. Meistens besteht schon eine Bezeichnung (z.B. der Flurname). Die Art oder Funktion gehören nicht zur Namensbezeichnung
       */
      Name_Nummer : TEXT*40;
      Geometrie : MANDATORY IPW_GWP_Basis.Polyline;
      Funktion : MANDATORY (
        unbekannt,
        Fernwasserleitung,
        Zubringerleitung,
        Hauptleitung,
        Versorgungsleitung,
        Anschlussleitung(
          normal,
          gemeinsam
        ),
        Entlueftungsleitung,
        Hydrantenanschlussleitung,
        Entleerungsleitung,
        Quellleitung,
        Quellableitung,
        Interne_Leitung,
        Bau_Wasseranschluss
      );
      /** Die Angabe bezieht sich auf das Material, aus welchem das Objekt besteht
       */
      Material : MANDATORY (
        unbekannt,
        Faserzement(
          Faserzement,
          Asbestzement
        ),
        Zementrohr_Beton(
          unbekannt,
          armiert,
          nicht_armiert
        ),
        Guss(
          unbekannt,
          Grauguss,
          Guss_duktil,
          Ahrens_Guss
        ),
        Kunststoff(
          Epoxiharz,
          Glasfaserverstaerkter_Epoxiharz,
          Polypropylen,
          Polyvinylchlorid(
            unbekannt,
            GFK,
            PVC_hart,
            PVC_U,
            andere
          ),
          Polyethylen(
            unbekannt,
            HDPE,
            LDPE,
            MDPE,
            andere
          )
        ),
        Stahl(
          unbekannt,
          Mannesmann,
          nicht_rostbestaendig,
          rostbestaendig,
          verzinkt
        ),
        Steinzeug,
        Ton,
        Teuchel
      );
      /** Innenrohrdurchmesser der Leitung. Das Attribut muss bei bestehenden öffentlichen Trinkwasserleitungen immer ausgefüllt sein, falls der Wert bekannt ist. 
       */
      DurchmesserInnen : 0 .. 9999 [Units.mm];
      /** Aussendurchmesser für alle Kunststoffrohre
       */
      DurchmesserAussen : 0 .. 9999 [Units.mm];
      /** Das Attribut wird nicht ausgefüllt. Beim Durchmesser ist der Innendurchmesser gemeint
       */
      Durchmesser : 0 .. 9999 [Units.mm];
      /** Die Nennweite bezeichnet den standardisierten Innendurchmesser, wobei die tatsächlichen Innendurchmesser je nach Rohrmaterial und Herstellungsverfahren leicht variieren können
       */
      Nennweite : TEXT*10;
      /** Qualität des Trinkwassers in der Leitung
       */
      Wasserqualitaet : (
        unbekannt,
        Trinkwasser,
        Industriebrauchwasser,
        Rohwasser,
        andere
      );
      /** Definiert die Lagebestimmung
       */
      Lagebestimmung : MANDATORY (
        unbekannt,
        ungenau,
        genau,
        geortet
      );
      Status : MANDATORY IPW_GWP_Basis.Status;
      /** Jahr der Inbetriebsetzung (Schlussabnahme), bei unbekanntem Wert ist der Wert 1800 zu wählen (der tiefste Wert des Wertebereichs)
       */
      Baujahr : MANDATORY IPW_GWP_Basis.Jahr;
      /** Art zur Verbindung der Leitungselemente (Rohrstangen)
       */
      Verbindungsart : (
        unbekannt,
        Muffen(
          unbekannt,
          gesteckt,
          geflanscht,
          geschweisst,
          geschraubt,
          gestemmt,
          spiegelgeschweisst,
          elektrogeschweisst,
          Bleimuffen
        ),
        Stopfbuechse,
        Gewinde,
        Eternitkupplung,
        Straubkupplung,
        Gibaultkupplung
      );
      Aussenbeschichtung : (
        unbekannt,
        keine,
        Bitumenschicht,
        Chlor_Kautschuk,
        Faserzementmoertel,
        Zink_Epoxi_Polyrethan,
        Polyethylen_verstaerkt,
        Polyethylen_Folie,
        Jute,
        Petrolatumband,
        Kunststoffband,
        Bitumenband,
        Schrumpffolie
      );
      Innenbeschichtung : (
        unbekannt,
        keine,
        Polyurethan(
          normal,
          Ecopur,
          andere
        ),
        Polyethylen,
        Zementmoertel,
        emailiert,
        bitumiert,
        Zinkstein_Epoxi
      );
      /** Art der Verlegung
       */
      Verlegeart : (
        unbekannt,
        offener_Graben,
        grabenlos,
        Rohr_pressen,
        freiliegend
      );
      Schubsicherung : (
        unbekannt,
        aussen,
        innen,
        keine,
        Duekerring,
        DueRo_Schubsicherung,
        Klemmschelle
      );
      /** Überdeckung der Leitung gemessen von OK Terrain bis OK Rohr
       */
      Ueberdeckung : IPW_GWP_Basis.Ueberdeckung;
      /** Angabe, ob die Leitung saniert wurde. Falls sie saniert wurde, wird angegeben, welche Art von Sanierung vorgenommen wurde
       */
      Sanierung_Erneuerung : (
        keine,
        PE_Rohrrelining,
        Zement_Auskleidung,
        Epoxi_Auskleidung,
        Muffenabdichtung,
        Schlauch_Auskleidung,
        Stahl_Relining,
        andere
      );
      /** Material, mit dem die Leitung im Graben umhüllt wurde
       */
      Bettung : (
        unbekannt,
        Betonkies,
        Beton,
        Sand,
        Recyclingmaterial,
        Fliess
      );
      /** Korrosionsschutz der Leitung mit kathodischem Schutzverfahren
       */
      Kathodischer_Schutz : (
        unbekannt,
        keiner,
        Gleichstrom,
        Wechselstrom,
        Opferanode
      );
      /** Ein Objektidentifikator (OID) besteht aus einem Präfix- und einem Postfix-Anteil und hat als Gesamtheit eine Länge von 16 alphanumerischen Stellen. Das Präfix bleibt gleich und identifiziert den Erfasser (Firma etc.). Weitere Infos siehe Merkblatt 2015 zur SIA Norm 405, Kapitel 2.1.3.8 Objektidentifikatoren (OID). 
       * Beim Status "tot" oder "ausser_Betrieb" muss die entsprechende Druckzone mit "Alt_ausser_Betrieb" angegeben werden.
       */
      Druckzone : MANDATORY TEXT*30;
      /** Zulässiger maximaler dynamischer Druck der Leitung
       */
      Zulaessiger_Bauteil_Betriebsdruck : 0.0 .. 99.9 [Units.bar];
      /** Dieser Wert ist nicht auszufüllen
       */
      Betriebsdruck : 0.0 .. 99.9 [Units.bar];
      /** Wandrauhigkeit gemäss Prandtl - Colebrook
       */
      Hydraulische_Rauheit : 0.00 .. 9999.99;
      /** Numerisch berechneter Wert
       */
      Laenge : MANDATORY 0 .. 999999 [Units.mm];
      /** Das Attribut wird nicht ausgefüllt
       */
      Unterhalt : TEXT*30;
      /** Beschreibung des Zustands
       */
      Zustand : MANDATORY TEXT*40;
      /** Innendurchmesser bei geplantem Ersatz gemäss GWP bei gleicher Leitungsführung
       */
      Durchmesser_geplant : 0 .. 9999 [Units.mm];
      /** Zur Definition der Genauigkeit des Attributes DurchmesserInnen
       */
      Genauigkeit_Durchmesser : (
        bestimmt,
        geschaetzt
      );
      /** Das Attribut weist die Beitragsberechtigung des Bauwerks für den Planungszustand aus. Die Beitragsberechtigung ist unabhängig vom Beitragssatz der Wasserversorgung
       */
      Subventionsberechtigung : MANDATORY (
        nicht_berechtigt,
        teilweise_berechtigt,
        vollstaendig_berechtigt,
        unbestimmt
      );
      Trinkwasser : MANDATORY IPW_GWP_Basis.JaNein;
      /** Diese Angabe bezieht sich auf das Basisjahr für die Berechnung des Wiederbeschaffungswerts (WBW). Basisjahr und Wiederbeschaffungswert müssen beide ausgefüllt sein
       */
      WBW_Basisjahr : IPW_GWP_Basis.Jahr;
      /** Diese Angabe bezieht sich auf den Wiederbeschaffungswert (WBW) eines Bauwerks und bezieht sich auf das Basisjahr. Basisjahr und Wiederbeschaffungswert müssen beide ausgefüllt sein
       */
      Wiederbeschaffungswert : 0.00 .. 99999999.99 [Units.CHF];
    END Leitung;

