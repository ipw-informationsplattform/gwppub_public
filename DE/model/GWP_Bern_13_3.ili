      Stoerreserve_geplant : 0.00 .. 10000000.00 [Units.m3];
      /** Entspricht der Störreserve im Istzustand gemäss GWP (zur Abdeckung von Störfällen wie Stromausfälle, Pumpendefekte, Brunnstubenreinigungen, Leitungsbrüche etc.)
       */
      Stoerreserve_Ist : 0.00 .. 10000000.00 [Units.m3];
      /** Entspricht der erforderlichen Störreserve im Istzustand gemäss GWP (zur Abdeckung von Störfällen wie Stromausfälle, Pumpendefekte, Brunnstubenreinigungen, Leitungsbrüche etc.)
       */
      Stoerreserve_Ist_benoetigt : 0.00 .. 10000000.00 [Units.m3];
