      Annee_du_releve : MANDATORY 2000 .. 2100 [PIE_PGA_Base.y];
      /** Consommation journalière maximale moyenne l’année du relevées
       */
      Consommation_maximale_reelle : MANDATORY PGA_Berne_13_3.PGA_Installations_SE.S_Consommation;
      /** Consommation journalière moyenne l’année du relevé
       */
      Consommation_moyenne_reelle : MANDATORY PGA_Berne_13_3.PGA_Installations_SE.S_Consommation;
    END Consommation_d_eau_volume;

