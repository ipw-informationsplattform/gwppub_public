      DeclencheurRef (EXTERNAL) -- {0..1} PIE_PGA_Base.PGA_Administration.Organisation;
      Lieu_de_fuite__DeclencheurAssocRef -- {0..*} Lieu_de_fuite;
    END Lieu_de_fuite_DeclencheurAssoc;

    CLASS Noeud_de_conduite (ABSTRACT)
    EXTENDS Cadastre_des_ouvrages_BaseClass =
      Geometrie : MANDATORY PIE_PGA_Base.CoordNat;
      /** Pour les installations suivantes, l’orientation précise est souhaitée : Installation.Genre = Puits.puits_d_alimentation_en_eau_potable.*, Chambre.chambre_de_reduction_de_pression, Chambre.chambre_de_liaison.
       */
      SymboleOri : MANDATORY PIE_PGA_Base.Orientation;
