    CLASS Organe_de_fermeture
    EXTENDS Noeud_de_conduite =
      /** L’attribut «Nom_numero» correspond à une désignation de nom. Son utilisation doit être uniforme. Le plus souvent, une désignation (p. ex. nom local) existe déjà. Le genre et la fonction ne font pas partie de la désignation du nom.
       */
      Nom_numero : TEXT*40;
      /** Genre d’organe de fermeture
       */
      Genre : (
        Information_non_connue,
        Vanne(
          genre_non_connue,
          vanne_normale(
            piece_en_T_combinee_une_sortie,
            piece_en_T_combinee_trois_sorties,
            piece_en_T_combinee_quatre_sorties
          ),
          vanne_de_raccordement,
          vanne_hydrant,
          vanne_en_ligne,
          vanne_de_separation,
          vanne_de_zone,
          vanne_de_prise_en_charge,
          vanne_de_vidange
        ),
        Clapet(
          genre_non_connu,
          clapet_normal,
          clapet_en_ligne,
          clapet_de_zone
        )
      );
      Etat_de_la_connexion : (
        Information_non_connue,
        Connexion_ouverte,
        Connexion_fermee
      );
      Commande : (
        Information_non_connue,
        Aucune_commande,
        Commande_motorisee(
          genre_non_connu,
          avec_commande_a_distance,
          sans_commande_a_distance
        ),
        Commande_mecanique
      );
      /** L’indication porte sur le matériau dans lequel l’objet est fabriqué.
       */
      Materiau : (
        Matiere_plastique,
        Information_non_connue,
        Acier,
        Fonte
      );
      Pression_de_fonctionnement_admissible : 0.0 .. 99.9 [Units.bar];
      Largeur_nominale : TEXT*10;
