    EXTENDS PIE_PGA_Base.PGA_TextePos =
    END Noeud_hydraulique_texte;

    ASSOCIATION Noeud_hydraulique_texteAssoc =
      /** Clé étrangère relative à la classe «Noeud_hydraulique»
       */
      Noeud_hydrauliqueRef -<#> {1} Noeud_hydraulique;
      Texte -- {0..*} Noeud_hydraulique_texte;
    END Noeud_hydraulique_texteAssoc;

    /** La classe «Tronçon_hydraulique» n’est généralement pas saisie, elle ne fait pas partie des données fournies à la plateforme d’information Eau (PIE).
     */
    CLASS Troncon_hydraulique
    EXTENDS Cadastre_des_ouvrages_BaseClass =
      /** L’attribut «Nom_numero» correspond à une désignation de nom. Son utilisation doit être uniforme. Le plus souvent, une désignation (p. ex. nom local) existe déjà. Le genre et la fonction ne font pas partie de la désignation du nom.
       */
      Nom_numero : MANDATORY TEXT*40;
      /** Valeur calculée
       */
      Reference_longueur : MANDATORY 0 .. 999999 [Units.mm];
      /** Rugosité de la paroi conformément à la formule de Prandtl-Colebrook
       */
      Reference_rugosite : 0 .. 9999;
      /** Valeur calculée (en général, valeur moyenne) du diamètre des différents éléments de la conduite
       */
      Reference_diametre : MANDATORY 0 .. 9999 [Units.mm];
      Consommation : 0 .. 99999999 [PIE_PGA_Base.m3d];
      /** Volume conforme au calcul de réseau (situation réelle, valeur statique)
       */
      Debit : 0.00 .. 9999.99 [PIE_PGA_Base.m3d];
      Vitesse_ecoulement : 0.000 .. 9999.999 [PIE_PGA_Base.ms];
      /** Description de la condition
       */
      Condition : TEXT*40;
      UNIQUE Nom_numero, ExploitantRef;
    END Troncon_hydraulique;

