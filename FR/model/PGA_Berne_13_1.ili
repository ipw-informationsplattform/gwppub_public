    END Troncon_hydraulique_texteAssoc;

    ASSOCIATION Troncon_hydraulique_duNoeudAssoc =
      /** Clé étrangère relative à la classe «Noeud_hydraulique»
       */
      duNoeudRef -- {1} Noeud_hydraulique;
      Troncon_hydraulique_auNoeudAssocRef -- {0..*} Troncon_hydraulique;
    END Troncon_hydraulique_duNoeudAssoc;

    ASSOCIATION Troncon_hydraulique_auNoeudAssoc =
      /** Clé étrangère relative à la classe «Noeud_hydraulique»
       */
      auNoeudRef -- {1} Noeud_hydraulique;
      Troncon_hydraulique_auNoeudAssoc -- {0..*} Troncon_hydraulique;
    END Troncon_hydraulique_auNoeudAssoc;

    CLASS Conduite
    EXTENDS Cadastre_des_ouvrages_BaseClass =
      /** L’attribut «Nom_numero» correspond à une désignation de nom. Son utilisation doit être uniforme. Le plus souvent, une désignation (p. ex. nom local) existe déjà. Le genre et la fonction ne font pas partie de la désignation du nom.
       */
      Nom_numero : TEXT*40;
      Geometrie : MANDATORY PIE_PGA_Base.Polyligne;
      Fonction : MANDATORY (
        Information_non_connue,
        Conduite_longue_distance,
        Conduite_d_amenee,
        Conduite_principale,
        Conduite_de_distribution,
        Conduite_de_branchement(
          normale,
          commune
        ),
        Purge_mise_a_air,
        Conduite_hydrant,
        Conduite_de_vidange,
        Conduite_de_source,
        Conduite_de_depart,
        Conduite_interne,
        Chantier_raccordement_en_eau
      );
      /** L’indication porte sur le matériau dans lequel l’objet est fabriqué.
       */
      Materiau : MANDATORY (
        Information_non_connue,
        Fibrociment(
          fibrociment,
          amiante_ciment
        ),
        Tube_en_ciment_beton(
          type_non_connu,
          arme,
          non_arme
        ),
        Fonte(
          inconnue,
          fonte_grise,
          fonte_ductile,
          fonte_ahrens
        ),
        Matiere_synthetique(
          resine_epoxy,
          resine_epoxy_renforcee_par_des_fibres_de_verre,
          polypropylene,
          chlorure_de_polyvinyle(
            type_non_connu,
            GFK,
            PVC_dur,
            PVC_type_non_connu,
            autre
          ),
          polyethylene(
            type_non_connu,
            HPDE,
            LPDE,
            MPDE,
            autre
          )
        ),
        Acier(
          type_non_connu,
          mannesmann,
          oxydable,
          inoxydable,
          galvanise
        ),
        Gres,
        Terre_cuite,
        Bois
      );
      /** Diamètre intérieur de la conduite. Cet attribut doit toujours être renseigné pour les conduites d'eau potable publiques existantes, si la valeur est connue.
       */
      Diametre_interieur : 0 .. 9999 [Units.mm];
      /** Diamètre extérieur de toutes les conduites en matière plastique PE
       */
      Diametre_exterieur : 0 .. 9999 [Units.mm];
      /** L’attribut n’est pas renseigné. Le terme «diamètre» correspond au diamètre intérieur.
       */
      Diametre : 0 .. 9999 [Units.mm];
      /** La largeur nominale désigne le diamètre intérieur standardisé, lequel peut légèrement varier selon la matière première et le procédé de fabrication utilisés.
       */
      Largeur_nominale : TEXT*10;
      /** Qualité de l’eau potable dans la conduite
       */
      Qualite_de_l_eau : (
        Information_non_connue,
        Eau_potable,
        Eau_d_usage_industriel,
        Eau_brute,
        Autre
      );
      /** Définit la détermination planimétrique
       */
      Determination_planimetrique : MANDATORY (
        Information_non_connue,
        Determination_imprecise,
        Determination_precise,
        Determination_relevee
      );
      Etat : MANDATORY PIE_PGA_Base.Etat;
      /** Année de mise en service (réception définitive). Si l’année n’est pas connue, sélectionner 1800 (valeur la plus faible de la plage de valeur)
       */
      Annee_de_construction : MANDATORY PIE_PGA_Base.Annee;
      /** Genre de raccordement entre les différents éléments de la conduite (tige tubulaire)
       */
      Genre_de_raccordement : (
        Information_non_connue,
        Manchon(
          type_non_connu,
          emboite,
          bride,
          soude,
          visse,
          enfonce,
          soude_au_miroir,
          electrosoude,
          manchon_a_plomb
        ),
        Presse_etoupe,
        Filetage,
        Raccord_eternit,
        Raccord_Straub,
        Raccord_Gibault
      );
      Isolation_exterieure : (
        Information_non_connue,
        Aucune,
        Revetement_de_bitume,
        Caoutchouc_chlore,
        Mortier_de_fibrociment,
        Polyurethane_galvanise,
        Polyethylene_renforce,
        Film_de_polyethilene,
        Jute,
        Enveloppe_bitumineuse,
        Bande_matiere_synthetique,
        Bande_bitumineuse,
        Manchette_retractable
      );
      Isolation_interieure : (
        Information_non_connue,
        Aucune,
        Polyurethane(
          normal,
          ecopur,
          autre
        ),
        Polyethylene,
        Mortier_de_ciment,
        Emaillee,
        Bitumee,
        Zinc_epoxy
      );
      /** Mode de pose
       */
      Mode_de_pose : (
        Information_non_connue,
        Tranchee_ouverte,
        Sans_tranchee,
        Compression,
        Pose_libre
      );
      Assurance_contre_la_poussee : (
        Information_non_connue,
        Assurance_exterieure,
        Assurance_interieure,
        Aucune,
        Joint_Duker,
        Joint_Duro,
        Anneau_de_verrouillage
      );
      /** Couverture de la conduite mesurée entre le niveau du terrain et le bord supérieur de la conduite
       */
      Couverture : PIE_PGA_Base.Couverture;
      /** Information indiquant si la conduite a été réhabilitée. Le cas échéant, le type de réhabilitation concerné est précisé.
       */
      Rehabilitation_renovation : (
        Aucune_rehabilitation,
        Gainage_interieur_PE,
        Revetement_ciment,
        Revetement_epoxy,
        Manchon_etanche,
        Tubage,
        Gainage_acier,
        Autre
      );
      /** Materiau d’enrobage de la conduite dans la tranchée
       */
      Lit_de_pose : (
        Information_non_connue,
        Gravier_a_beton,
        Beton,
        Sable,
        Materiau_de_recyclage,
        Geotextile
      );
      /** Protection anti-corrosion de la conduite par un procédé de protection cathodique
       */
      Protection_cathodique : (
        Information_non_connue,
        Aucune,
        Courant_continu,
        Courant_alternatif,
        Anode_sacrificielle
      );
      /** Les différentes zones de pression des ouvrages sont représentées avec des couleurs distinctes sur les plans de la plateforme d’information Eau (PIE). Par principe, seul le nom est inscrit (p. ex. «Village» en lieu et place de «Zone_de_pression Village»).
       * Pour les états «Abandonne» ou «Hors_service», il convient d’indiquer la zone de pression correspondante au moyen de «Vetuste_hors_service».
       */
      Zone_de_pression : MANDATORY TEXT*30;
      /** Pression dynamique maximale admissible de la conduite
       */
      Pression_de_fonctionnement_admissible : 0.0 .. 99.9 [Units.bar];
      /** Cet attribut ne doit pas être renseigné.
       */
      Pression_d_exploitation : 0.0 .. 99.9 [Units.bar];
      /** Rugosité de la paroi conformément à la formule de Prandtl-Colebrook
       */
      Rugosite_hydraulique : 0.00 .. 9999.99;
      /** Valeur calculée numériquement
       */
      Longueur : MANDATORY 0 .. 999999 [Units.mm];
      /** L’attribut n’est pas renseigné.
       */
      Entretien : TEXT*30;
      /** Description de la condition
       */
      Condition : MANDATORY TEXT*40;
      /** Diamètre intérieur pour tout remplacement planifié selon le PGA sans modification du tracé.
       */
      Diametre_planifie : 0 .. 9999 [Units.mm];
      /** Pour définir le degré de précision de l’attribut «Diamètre intérieur».
       */
      Precision_sur_les_diametres : (
        Diametre_determine,
        Diametre_estime
      );
      /** L’attribut indique le droit aux subventions de l’ouvrage en fonction de l’état de la planification. Le droit aux subventions ne dépend pas du taux de subvention de l’alimentation en eau.
       */
      Droit_aux_subventions : MANDATORY (
        Non_eligible,
        Partiellement_eligible,
        Entierement_eligible,
        Droit_non_determine
      );
      Eau_potable : MANDATORY PIE_PGA_Base.OuiNon;
      /** Cette information indique l’année de référence pour le calcul de la valeur de remplacement Il importe de renseigner à la fois l’année de référence et la valeur de remplacement.
       */
      VRADE_valeur_de_remplacement_annee_de_reference : PIE_PGA_Base.Annee;
      /** Cette information concerne la valeur de remplacement d’un ouvrage et tient compte de l’année de référence. Il importe de renseigner à la fois l’année de référence et la valeur de remplacement.
       */
      Valeur_de_remplacement : 0.00 .. 99999999.99 [Units.CHF];
    END Conduite;

    ASSOCIATION Conduite_ConcessionnaireAssoc =
      /** Cette information se réfère à l’organisation qui est propriétaire de la concession. La clé étrangère relative au concessionnaire dans la classe «Organisation» est utilisée.
       */
      ConcessionnaireRef (EXTERNAL) -- {0..1} PIE_PGA_Base.PGA_Administration.Organisation;
      Conduite_ConcessionnaireAssocRef -- {0..*} Conduite;
    END Conduite_ConcessionnaireAssoc;

