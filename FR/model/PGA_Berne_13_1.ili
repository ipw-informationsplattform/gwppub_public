    END Cadastre_des_ouvrages_BaseClass_ExploitantAssoc;

    /** La classe «Noeud_hydraulique» n’est généralement pas saisie, elle ne fait pas partie des données fournies à la plateforme d’information Eau (PIE).
     */
    CLASS Noeud_hydraulique
    EXTENDS Cadastre_des_ouvrages_BaseClass =
      /** L’attribut «Nom_numero» correspond à une désignation de nom. Son utilisation doit être uniforme. Le plus souvent, une désignation (p. ex. nom local) existe déjà. Le genre et la fonction ne font pas partie de la désignation du nom.
       */
      Nom_numero : MANDATORY TEXT*40;
      Geometrie : PIE_PGA_Base.CoordNat;
      Type_de_noeud : (
        Noeud_normal,
        Information_non_connue,
        Point_injection
      );
      Consommation : 0 .. 99999999 [PIE_PGA_Base.m3d];
      Pression : 0.0 .. 99.9 [Units.bar];
      UNIQUE Nom_numero, ExploitantRef;
    END Noeud_hydraulique;

    /** La classe «Noeud_hydraulique_texte» n’est généralement pas saisie, elle ne fait pas partie des données fournies à la plateforme d’information Eau (PIE).
     */
